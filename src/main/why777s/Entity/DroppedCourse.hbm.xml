<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.DroppedCourse" table="droppedCourse" schema="mySchool">
        <composite-id mapped="true" class="Entity.DroppedCoursePK">
            <key-property name="sid">
                <column name="sid" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="cid">
                <column name="cid" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="tid">
                <column name="tid" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="semester">
                <column name="semester" sql-type="varchar(45)" length="45"/>
            </key-property>
        </composite-id>
        <many-to-one name="courseByCid" class="Entity.Course">
            <column name="cid"/>
        </many-to-one>
    </class>
</hibernate-mapping>