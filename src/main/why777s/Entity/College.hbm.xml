<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.College" table="college" schema="mySchool">
        <id name="yxh">
            <column name="yxh" sql-type="varchar(2)" length="2"/>
        </id>
        <property name="yname">
            <column name="yname" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>