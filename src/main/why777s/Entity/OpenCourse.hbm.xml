<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.OpenCourse" table="openCourse" schema="mySchool">
        <composite-id mapped="true" class="Entity.OpenCoursePK">
            <key-property name="cid">
                <column name="cid" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="tid">
                <column name="tid" sql-type="varchar(8)" length="8"/>
            </key-property>
            <key-property name="semester">
                <column name="semester" sql-type="varchar(45)" length="45"/>
            </key-property>
        </composite-id>
        <property name="time">
            <column name="time" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>